	  <%= csrf_meta_tags %>
	</head>
	<body>
		<header>
			<nav class="navbar navbar-default">
			  <div class="container-fluid">
			    <div class="navbar-header">
			      <a class="navbar-brand" href="#">G4</a>
			    </div>
			    <ul class="nav navbar-nav">
			      <li class="active"><a href="#">Dashboard</a></li>
			    </ul>
			  </div>
			</nav>
		</header>
		<main>
			<div class="row">
				<div class="col-md-2">
				</div>
				<div class="col-md-8">
					<%= yield %>
				</div>
				<div class="col-md-2">
				</div>
			</div>
		</main>
	</body>
</html>
